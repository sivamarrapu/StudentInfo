student_actual_dateof_birth varchar(50)

)



create table students_details(

rollno varchar(30) primary key,

name varchar(200),

section varchar(20),

mobile_message varchar(15),

eamcet_hallticket varchar(25),

eamcet_rank varchar(25)

)


